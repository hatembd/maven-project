pipeline {
    agent any

    stages {
        stage('pull request') {
            steps {
                echo 'Hello git'
            }
        }
         stage('build') {
            steps {
                echo 'Hello build'
            }
        }
         stage('test') {
            steps {
                echo 'succes test'
            }
         }
             stage('deploy') {
            steps {
                echo 'deploy'
            }
        }
        
    }
}
